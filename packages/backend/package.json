{
  "name": "@sonic-atlas/backend",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node check-node-version.js && node --env-file=../../.env --watch src/index.ts",
    "build": "tsc",
    "start": "node --env-file=../../.env dist/src/index.js",
    "test": "vitest",
    "check": "tsc --noEmit",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate"
  },
  "dependencies": {
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "drizzle-orm": "^1.0.0-beta.11-05230d9",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^3.0.1",
    "multer": "^2.0.2",
    "music-metadata": "^11.9.0",
    "postgres": "^3.4.7",
    "sharp": "^0.34.5",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mime-types": "^3.0.1",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.9.2",
    "@types/sharp": "^0.31.1",
    "drizzle-kit": "^1.0.0-beta.11-05230d9",
    "typescript": "^5.9.3",
    "vitest": "^4.0.5"
  },
  "engines": {
    "node": ">=22.18.0"
  },
  "imports": {
    "#db/*": "./db/*.ts"
  }
}
